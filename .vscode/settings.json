{
    "vsicons.presets.angular": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules/": true,
        "**/*.js": {"when": "$(basename).ts"},
        "**/*.map": {"when": "$(basename)"}
    }
}